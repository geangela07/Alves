class Pokemon:                      #Definição de classe pokemon geral
    def __init__(self,nome,level,tipo,hp):
        self.nome = nome
        self.level = level
        self.tipo = tipo
        self.hp = hp

class pokemonAquatico(Pokemon):           #Definição de classe pokemon especifico
    def __init__(self,nome,level,tipo,hp):
        super().__init__(self,nome,level,tipo,hp)
        self.nome = nome
        self.level = level
        self.tipo = tipo
        self.hp = hp


class pokemonEletrico(Pokemon):
    def __init__(self,nome,level,tipo,hp):
        super().__init__(self,nome,level,tipo,hp)
        self.nome = nome
        self.level = level
        self.tipo = tipo
        self.hp = hp

class pokemonFogo(Pokemon):
    def __init__(self,nome,level,tipo,hp):
        super().__init__(self,nome,level,tipo,hp)
        self.nome = nome
        self.level = level
        self.tipo = tipo
        self.hp = hp
